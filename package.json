    "test:integration": "playwright test",
    "test:unit": "vitest"
  },
  "devDependencies": {
    "@friendofsvelte/toggle": "^0.0.2",
    "@iconify/json": "^2.2.239",
    "@playwright/test": "^1.28.1",
